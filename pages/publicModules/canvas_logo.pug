if !isIncluded('canvas_logo.pug')
  -
    var quotes = [
      '好读书，不求甚解。',
      '实践和理论相结合。',
      '同学们，你们是祖国的栋梁，要注意实验安全！',
      '华语世界最纯粹的科技/文艺/爱好社区, Since 2001.',
      '从这里开始你的发现之旅。',
      '发展科技爱好，倡导科学理性。',
      '那榆蔭下的一潭，不是清泉，是天上虹。',
      '揉碎在浮藻間，沉澱著彩虹似的夢。',
      '发展体育运动，提高人民体质',
      '科创精神就是向李小平同志学习。',
      '天行健，君子以自强不息。',
      '地势坤，君子以厚德载物。',
      '自强不息，厚德载物；独立精神，自由思想。',
      '博学、审问、慎思、明辨、笃行。',
      '思想自由，兼容并包。',
      '博学而笃志，切问而近思。'
    ];

  mixin renderCanvasLogo(logosUrl)
    -var logosLength = logosUrl.length;
    -var logoUrl = logosUrl[Math.floor(Math.random() * logosLength)];
    -var quote = quotes[Math.floor(Math.random()*quotes.length)];
    .canvas-logo
      img(src=logoUrl title=quote)
