  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js ",
    "serve": "node ./server.js",
    "test": "mocha -t 10000",
    "watch": "concurrently -p \"[{name}]\" -n \"node,pages\" -c \"green,cyan\" \"npm run watch-node\" \"npm run watch-pages\"",
    "watch-node": "nodemon --ignore pages/ --ignore dist/ --delay 1000ms server.js",
    "watch-pages": "concurrently -p \"[{name}]\" -n \"css,js,assets\" -c \"green,cyan,blue\" \"gulp watch:css\" \"gulp watch:js\" \"gulp watch:assets\"",
    "build-pages": "gulp compile:css && gulp compile:js && gulp copy:assets",
    "build-pages-p": "cross-env NODE_ENV=production npm run build-pages",
    "clear": "rimraf dist"
  },
